Package: PhaseTypeGenetics
Type: Package
Title: The Phase-Type Distribution with Applications in Population Genetics
Version: 0.5.1
Authors@R: c(
    person("Peter", "Krarup", email = "peter.krarup@post.au.dk", role = "aut"),
